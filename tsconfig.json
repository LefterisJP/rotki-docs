{
  "compilerOptions": {
    "target": "es2020",
    "baseUrl": ".",
    "module": "esnext",
    "moduleResolution": "Bundler",
    "paths": {
      "@/*": ["./*"]
    },
    "types": ["vite/client"]
  },
  "include": [
    "**/*.ts",
    "**/*.d.ts",
    "**/*.vue",
    ".vitepress/**/*.ts",
    ".vitepress/**/*.mts"
  ]
}
